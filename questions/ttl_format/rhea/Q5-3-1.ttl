@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:gHhZRc a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q5-3-1" ;
    rdfs:comment "Is the reaction shown in rhea:10024 stoichiometrically correct?"@en ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: Is the reaction shown in rhea:10024 stoichiometrically correct?
# Parameter: Reaction: (example: rhea:10024)

PREFIX rhea: <http://rdf.rhea-db.org/>

SELECT DISTINCT ?reaction_is_chemically_balanced 
WHERE {
    VALUES ?Reaction { rhea:10024 }
    ?Reaction rhea:isChemicallyBalanced ?reaction_is_chemically_balanced .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
