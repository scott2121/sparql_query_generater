@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:l4FtRL a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q3-1-1" ;
    rdfs:comment "Please tell me the RHEA IDs of reactions involving the L-lysyl-[histone] molecule"@en ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: Please tell me the RHEA IDs of reactions involving the L-lysyl-[histone] molecule
# Parameter: compound_name: (example: L-lysyl-[histone])

PREFIX rhea: <http://rdf.rhea-db.org/>

SELECT DISTINCT ?Reaction 
WHERE {
    VALUES ?compound_name { "L-lysyl-[histone]" }
    ?Reaction rhea:side / rhea:contains / rhea:compound / rhea:name ?compound_name .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
