@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:ElZV2X a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q7-3-1" ;
    rdfs:comment "Please inform me about the verification status of the reaction related to rhea:10024."@en ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: Please inform me about the verification status of the reaction related to rhea:10024.
# Parameter: Reaction: (example: rhea:10024)

PREFIX rhea: <http://rdf.rhea-db.org/>

SELECT DISTINCT ?reaction_status 
WHERE {
    VALUES ?Reaction { rhea:10024 }
    ?Reaction rhea:status ?reaction_status .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
