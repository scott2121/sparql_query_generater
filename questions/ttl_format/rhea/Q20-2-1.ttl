@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:PZn1yw a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q20-2-1" ;
    rdfs:comment "What is the RHEA identifier of the reaction that includes the L-lysyl-[histone] molecule, and is it a transport reaction?"@en ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: What is the RHEA identifier of the reaction that includes the L-lysyl-[histone] molecule, and is it a transport reaction?
# Parameter: compound_name: (example: L-lysyl-[histone])

PREFIX rhea: <http://rdf.rhea-db.org/>

SELECT DISTINCT ?Reaction ?reaction_is_transport 
WHERE {
    VALUES ?compound_name { "L-lysyl-[histone]" }
    ?Reaction rhea:isTransport ?reaction_is_transport ;
        rhea:side / rhea:contains / rhea:compound / rhea:name ?compound_name .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
